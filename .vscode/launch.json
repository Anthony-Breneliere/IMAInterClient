{
    "version": "0.2.0",



    "configurations": [
        {
            "name": "Chrome ImaInter",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:4512/#/",
            "sourceMaps": true,
            "sourceMapPathOverrides": { 
                "webpack:/*": "${webRoot}/*"
            },
            "webRoot": "${workspaceFolder}",
            "skipFiles": [
                "zone.js"
            ],
        },

        {
            "name": "Chrome TestPage",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:4512/#/test",
