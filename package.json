{
  "name": "ima-intervention",
  "version": "0.2.0",
  "description": "Client Station de gestion et du suivi des interventions",
  "scripts": {
    "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "start2": "ng serve -port 3001 -dev",
    "start2_pro": "ng serve -port 3001 -prod",
    "docker-build": "docker build -t ng2-imainter .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-imainter",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "lite": "lite-server",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "test-once": "tsc && karma start karma.conf.js --single-run",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "webdriver:update": "webdriver-manager update",
    "serve:bundle": "lite-server -c bs-config.bundle.json",
    "ngc": "ngc -p tsconfig-aot.json",
    "bundle": "ngc -p tsconfig-aot.json && node resources_copy bundle && rollup -c rollup-config.js",
    "publish": "node publish"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "@angular/upgrade": "^2.4.0",
    "angular-in-memory-web-api": "^0.2.4",
    "angular2-contextmenu": "^0.8.2",
    "angular2-moment": "^1.0.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "gridstack": "^0.2.6",
    "jquery": "^3.1.1",
    "jquery-ui-dist": "^1.12.1",
    "lodash": "^4.17.4",
    "material-design-icon-fonts": "~3.0.1",
    "moment": "^2.17.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.0.3",
    "signalr": "^2.2.1",
    "systemjs": "0.19.40",
    "typescript-collections": "^1.2.3",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.0",
    "@angular/platform-server": "^2.4.0",
    "@types/angular-signalr-hub": "^1.5.31",
    "@types/gridstack": "0.0.34",
    "@types/jasmine": "^2.5.36",
    "@types/jquery": "^2.0.39",
    "@types/lodash": "^4.14.54",
    "@types/node": "^7.0.5",
    "@types/selenium-webdriver": "^2.53.40",
    "@types/signalr": "^2.2.33",
    "canonical-path": "0.0.2",
    "concurrently": "^3.1.0",
    "http-server": "^0.9.0",
    "jasmine-core": "~2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "protractor": "4.0.9",
    "rimraf": "^2.5.4",
