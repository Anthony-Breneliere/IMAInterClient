{
    "name": "ima-intervention",
    "version": "0.2.0",
    "description": "Client Station de gestion et du suivi des interventions",
    "scripts": {
        "start": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
        "start2": "ng serve -port 3001 -dev",
        "start2_pro": "ng serve -port 3001 -prod",
        "docker-build": "docker build -t ng2-imainter .",
        "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-imainter",
        "pree2e": "npm run webdriver:update",
        "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
        "lint": "tslint ./app/**/*.ts -t verbose",
        "lite": "lite-server",
        "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
        "test-once": "tsc && karma start karma.conf.js --single-run",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "webdriver:update": "webdriver-manager update",
        "serve:bundle": "lite-server -c bs-config.bundle.json",
        "ngc": "ngc -p tsconfig-aot.json",
        "bundle": "ngc -p tsconfig-aot.json && node resources_copy bundle && rollup -c rollup-config.js",
        "publish": "node publish"
    },
    "keywords": [],
    "author": "Anthony Breneliere IMA Protect/TOT Concept",
    "license": "MIT",
    "dependencies": {
        "@angular/common": "^4.1.1",
        "@angular/compiler": "^4.1.1",
        "@angular/core": "^4.1.1",
        "@angular/forms": "^4.1.1",
        "@angular/http": "^4.1.1",
        "@angular/platform-browser": "^4.1.1",
        "@angular/platform-browser-dynamic": "^4.1.1",
        "@angular/router": "^4.1.1",
        "@angular/upgrade": "^4.1.1",
        "angular2-contextmenu": "^0.8.2",
        "angular2-moment": "^1.3.3",
        "bootstrap": "^3.3.7",
        "core-js": "^2.4.1",
        "gridstack": "^0.2.6",
        "jquery": "^3.1.1",
        "jquery-ui-dist": "^1.12.1",
        "lodash": "^4.17.4",
        "material-design-icon-fonts": "~3.0.1",
        "moment": "^2.18.1",
        "reflect-metadata": "^0.1.10",
        "rxjs": "5.3.1",
        "signalr": "^2.2.1",
        "systemjs": "0.20.12",
        "typescript-collections": "^1.2.3",
        "zone.js": "^0.8.10"
    },
    "devDependencies": {
